# Windowstast + shift + enter
"gnome-terminal"
  mod4 + shift + Return

# Torrent mappe
#"nautilus /home/`whoami`/Torrents"
#  mod4 + shift + t

# Download mappe
#"nautilus /home/`whoami`/Downloads"
#   mod4 + shift + d

# Log ud af gnome (og ind i xmonad) med samme key som det gøres fra xmonad
"gnome-session-save --logout"
  mod4 + shift + q

# Lås skærmen
"gnome-screensaver-command -l"
  mod4 + z

# Tag et screenshot og læg det i hjemmemappen
"scrot"
  Print

# Skru op for lyden
"soundcontroller up"
  XF86AudioRaiseVolume

# Skru ned for lyden
"soundcontroller down"
  XF86AudioLowerVolume

# Mute/unmute lyden
"soundcontroller toggle"
  XF86AudioMute

# Skru helt ned for lyden
"soundcontroller silence"
  mod4 + shift + m

# change sound card
"soundcontroller changesound"
  mod4 + s

# Lock screen and suspend computer
"sleep.sh"
  XF86Sleep

# Start et program
"dmenu_run -nb '#1a1a1a' -nf '#ffffff' -sb '#aecf96' -sf black -p '>'"
  mod4 + p
