"gnome-terminal"
  mod4 + shift + Return

"nautilus /home/`whoami`/Torrents"
  mod4 + shift + t

"nautilus /home/`whoami`/Downloads"
   mod4 + shift + d

# Locks the screen
"gnome-screensaver-command -l"
  mod4 + z

# Takes a screenshot and saves it in the home dir
"scrot"
  Print

# Turn volume up
"soundcontroller up"
  XF86AudioRaiseVolume

# Turn volume down
"soundcontroller down"
  XF86AudioLowerVolume

# Mute/unmute volume
"soundcontroller toggle"
  XF86AudioMute

# Silence the volume
"soundcontroller silence"
  mod4 + XF86AudioLowerVolume

# Sets volume to 100%
"soundcontroller normal"
  mod4 + XF86AudioRaiseVolume

# change sound card
"soundcontroller changesound"
  mod4 + XF86AudioMute

# opens the dmenu launcher
"dmenu_run -nb '#1a1a1a' -nf '#ffffff' -sb '#aecf96' -sf black -p '>'"
  mod4 + p

# Google-chrome
"google-chrome"
  mod4 + n

# Google-chrome incognito
"google-chrome --incognito"
  mod4 + shift + n

# Genstarter dzen + indstiller sk√¶rme
"tvconnect; sleep 0.1; restartdzenheader"
  XF86Sleep
