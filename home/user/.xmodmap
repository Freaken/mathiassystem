! Det er mest fornuftigt, at lave et dollarstegn med shift, i stedet for med alt-gr
! Den er normalt:
! keycode  13 = 4 currency 4 currency dollar onequarter
keycode 13  = 4 dollar currency dollar cent sterling

! Jeg vil gerne have et euro-tegn på min 5-tast
keycode 14  = 5 percent 5 percent EuroSign yen

! Backspace i stedet for Caps Lock
keycode 66 = BackSpace
! keycode 66 = Caps_Lock

! Jeg vil gerne have Mode_switch på menu tasten
keycode 135  = Mode_switch

! Rydder modifiers
clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

! Shift og control på begge taster
add shift   = Shift_L Shift_R
add control = Control_L Control_R

! Venstre alt-tast
add mod1    = Alt_L  Meta_L
add mod2    = Num_Lock

! Intet lige nu
! add mod3    = 

! Windows tast
add mod4    = Super_L

! AltGr
add mod5    = ISO_Level3_Shift

